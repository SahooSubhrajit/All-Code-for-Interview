package com.nt.babluIT.ArrayCoding;

public class Test132 {

	public static void main(String[] args) {

	}
}

class Emp {
	int id;
	String name;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Emp(int id, String name) {
		this.id = id;
		this.name = name;
	}

}
