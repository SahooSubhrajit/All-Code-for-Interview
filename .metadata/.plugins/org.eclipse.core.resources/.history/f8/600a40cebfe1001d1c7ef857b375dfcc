package com.nt.babluIT.ArrayCoding;

import java.util.Arrays;

public class Test132 {

	private static String removeDuplicates(int[] array) {

		Arrays.sort(array);

		int length = array.length - 1;

		for (int i = 0; i < length; i++) {
			for (int j = i + 1; j < length; j++) {
				if (array[i] == array[j]) {
					array[j] = array[length];
					length--;
					j--;

				}
			}
		}

		int[] copyOf = Arrays.copyOf(array, length);
//		System.out.println(Arrays.toString(copyOf));
		return Arrays.toString(copyOf);

	}

	public static void main(String[] args) {
		System.out.println(removeDuplicates(new int[] { 4, 3, 2, 4, 9, 2 }));

	}
}
